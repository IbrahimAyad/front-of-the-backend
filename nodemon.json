{
  "watch": ["src"],
  "ext": "ts,js,json",
  "ignore": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "node_modules",
    "dist",
    "prisma/migrations",
    ".git",
    "*.log"
  ],
  "delay": 2000,
  "env": {
    "NODE_ENV": "development"
  },
  "verbose": false,
  "restartable": "rs"
} 
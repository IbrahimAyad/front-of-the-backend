{
  "name": "kct-menswear-backend",
  "version": "1.0.0",
  "description": "Luxury menswear tailoring business management system backend",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "next dev -p 8000",
    "build": "npx prisma generate && next build",
    "vercel-build": "node scripts/vercel-prebuild.js && next build",
    "postinstall": "node scripts/postinstall.js",
    "start": "next start -p 8000",
    "start:dev": "next dev -p 8000",
    "deploy": "prisma migrate deploy && prisma db seed",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "db:seed": "prisma db seed",
    "db:seed:production": "tsx prisma/seed-production.ts",
    "db:seed:complete": "tsx prisma/seed-production-complete.ts",
    "db:reset": "prisma migrate reset",
    "db:setup": "prisma migrate deploy && prisma db seed",
    "seed:customers": "tsx prisma/seed-customers.ts",
    "seed:profiles": "tsx prisma/populate-customer-profiles.ts",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:compare": "tsx tests/api-comparison/run-comparison.ts",
    "test:compare:watch": "nodemon --exec tsx tests/api-comparison/run-comparison.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@date-io/date-fns": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^6.7.1",
    "@fastify/multipart": "^6.0.1",
    "@fastify/rate-limit": "^8.1.1",
    "@fastify/redis": "^7.0.2",
    "@fastify/websocket": "^7.2.0",
    "@hello-pangea/dnd": "^16.6.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.1.1",
    "@mui/lab": "^7.0.0-beta.13",
    "@mui/material": "7.1.1",
    "@mui/types": "7.4.3",
    "@mui/x-date-pickers": "^8.5.2",
    "@prisma/client": "^5.6.0",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.42.0",
    "@sentry/profiling-node": "^9.42.0",
    "@sinclair/typebox": "^0.34.38",
    "@tanstack/react-query": "^5.80.7",
    "@types/ioredis": "^4.28.10",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.15.4",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "fastify": "^4.29.1",
    "fastify-plugin": "^4.5.1",
    "form-data": "^4.0.4",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.516.0",
    "multer": "^1.4.5-lts.1",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "node-fetch": "^2.7.0",
    "openai": "^5.3.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.3",
    "redis": "^4.6.10",
    "resend": "^4.7.0",
    "stripe": "^18.3.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.14.2",
    "yup": "^1.6.1",
    "zod": "^4.0.10"

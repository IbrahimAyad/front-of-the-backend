{
  "name": "kct-menswear-backend",
  "version": "1.0.0",
  "description": "Luxury menswear tailoring business management system backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:frontend": "vite --port 3001",
    "build": "prisma generate && tsc --project tsconfig.server.json",
    "build:backend": "prisma generate && tsc --project tsconfig.server.json",
    "build:frontend": "vite build",
    "build:production": "vite build --mode production",
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "preview": "vite preview",
    "postinstall": "prisma generate",
    "deploy": "prisma migrate deploy && prisma db seed",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@date-io/date-fns": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^6.7.1",
    "@fastify/multipart": "^6.0.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/websocket": "^7.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.1.1",
    "@mui/lab": "^7.0.0-beta.13",
    "@mui/material": "7.1.1",
    "@mui/types": "7.4.3",
    "@mui/x-date-pickers": "^8.5.2",
    "@prisma/client": "^5.6.0",
    "@sendgrid/mail": "^8.1.5",
    "@tanstack/react-query": "^5.80.7",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "fastify": "^4.29.1",
    "fastify-plugin": "^4.5.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.516.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.3",
    "redis": "^4.6.10",
    "ws": "^8.14.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/ws": "^8.5.8",
    "@vitejs/plugin-react": "^4.5.2",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vite": "^6.3.5"
  }
}
